class Solution {
    public int solution(int[] num_list) {
        int answer = 0;
        String a="";
        String b="";
        for(int i=0;i<num_list.length;i++){
            if(num_list[i]%2==0){  //num_list배열에 있는값 짝수일경우
                b+=num_list[i];
            }else{  //홀수일 경우
                a+=num_list[i];
            }
        }
        answer=Integer.parseInt(a)+Integer.parseInt(b);//int형으로 변환하여 더함.
        return answer;
    }
}
